{
	"manifest_version": 2,
	"name" : "EzyUI",
	"version" : "1.0",
	"description": "Content",

	"applications": {
		"gecko": {
				"id": "addon@example.com",
				"strict_min_version": "42.0"
			}
	},

	"icons": {
		"48": "icons/ezyui-48.png"
	},
  
	"permissions": [
		"activeTab"
	],

	"browser_action": {
		"default_icon": "icons/ezyui-32.png",
		"default_title": "EzyUI"
	},

	"web_accessible_resources": [
		"resources/ezyui.css",
		"resources/jquery-3.2.0.min.js"
	],
  
   "background": {
	"scripts": [
		"ezyui_bg.js"
	]
   }
   
}